void setup() {
    Serial.begin(9600);
    pinMode(3, OUTPUT);

    // initialize the oled
    if (!oled.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
        Serial.println(F("cap"));
        while (true);
    }
    delay(1000); // wait a little
    oled.clearDisplay(); // clearDisplay

    oled.setTextSize(3);
    oled.setTextColor(WHITE);
    oled.println("hiii :3");
    oled.display();
    delay(2000);
    analogWrite(3, 200);
}

void loop() {
  oled.clearDisplay();

  // display bitmap
  oled.drawBitmap(0, 0, epd_bitmap_suss, 128, 64, WHITE);
  oled.display();
  delay(1000);
  oled.clearDisplay();
  oled.drawBitmap(0, 0, epd_bitmap_suss2, 128, 64, WHITE);
  oled.display();
  delay(1000);   
}
